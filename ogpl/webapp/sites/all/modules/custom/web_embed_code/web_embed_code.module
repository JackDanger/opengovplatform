<?phpfunction web_embed_code_menu() {    $items['embed_preview'] = array(    'title' => 'embed preview',    'description' => 'Embed preview',    'page callback' => 'embed_preview',    'access callback' => true,    'access arguments' => true,    'type' => MENU_NORMAL_ITEM,    'weight' => 10,    );    return $items;}function embed_preview(){    $output = theme('web_embed_code', $_POST);    die($output);}function web_embed_code_theme() {    $theme = array(        'web_embed_code' => array(            'arguments' => array('data' => NULL),            'template' => 'web_embed_code',        )    );    return $theme;}?>